    <script>
        function work() {
            var par = document.getElementById("3");
            par.innerHTML = "work in the";
            alternate();
        }

        function alternate() {
            var image = documnet.getElementById("pic");
            var text = image.getAttribute("alt");
            var p4 = document.getElementById("4");
            p4.innerHTML = text;
        }
        window.onload = work;
    </script>
